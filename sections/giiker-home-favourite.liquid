<div class="giiker-home-favourite" style="background-color: {{ section.settings.background_color }}">
  <div class="favourite-title">{{ section.settings.title }}</div>

  <div class="favourite-swiper">
    <div class="swiper favourite">
      <div class="swiper-wrapper">
        {% for block in section.blocks %}
          <div class="favourite-item swiper-slide">
            <a href="{{ block.settings.product.url }}">
              {% if block.settings.product_image %}
                <img src="{{ block.settings.product_image | image_url: width: block.settings.product_image.width }}" 
                     alt="{{ block.settings.product.title | escape }}"
                     loading="eager"
                     width="{{ block.settings.product_image.width }}"
                     height="{{ block.settings.product_image.height }}">
              {% else  %}
                <img src="{{ block.settings.product.featured_image | image_url: width: block.settings.product.featured_image.width }}" 
                     alt="{{ block.settings.product.title | escape }}"
                     loading="eager"
                     width="{{ block.settings.product.featured_image.width }}"
                     height="{{ block.settings.product.featured_image.height }}">
              {% endif %}
                
            <div class="item-title ">
              {% if block.settings.product_title != blank %}
                {{ block.settings.product_title }}
              {% else %}
                {{ block.settings.product.title }}
              {% endif %}
            </div>
          </a>
          </div>
        {% endfor %}
      </div>
      <div class="swiper-pagination"></div>
    </div>
    {% comment %} <div class="swiper-button-prev">
      <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="118" height="118" viewBox="0 0 118 118" fill="none">
        <rect width="117.539" height="117.539" transform="matrix(1 0 0 -1 0 118)" fill="url(#pattern0_582_512)"/>
        <defs>
        <pattern id="pattern0_582_512" patternContentUnits="objectBoundingBox" width="1" height="1">
        <use xlink:href="#image0_582_512" transform="scale(0.00260417)"/>
        </pattern>
        <image id="image0_582_512" width="384" height="384" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
        </defs>
      </svg>
    </div>
    <div class="swiper-button-next">
      <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="118" height="118" viewBox="0 0 118 118" fill="none">
        <rect width="117.539" height="117.539" transform="matrix(1 0 0 -1 0 118)" fill="url(#pattern0_582_511)"/>
        <defs>
        <pattern id="pattern0_582_511" patternContentUnits="objectBoundingBox" width="1" height="1">
        <use xlink:href="#image0_582_511" transform="scale(0.00260417)"/>
        </pattern>
        <image id="image0_582_511" width="384" height="384" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
        </defs>
      </svg>
    </div> {% endcomment %}
  </div>
  {% comment %} <div class="favourite-background" >
    {{
      section.settings.background_image
      | image_url: width: section.settings.background_image.width
      | image_tag: fetchpriority: 'high', loading: 'lazy', class: 'image-tag', data-cc-animate: 'fade-in'
    }}
  </div> {% endcomment %}
  
</div>
<script>
  document.addEventListener('DOMContentLoaded', function() {
    const swiperProductFeatures = new Swiper('#shopify-section-{{ section.id }} .swiper', {
      slidesPerView: 'auto',
      spaceBetween: 15,
      loop: true,
      pagination: {
        el: '#shopify-section-{{ section.id }} .swiper-pagination',
        clickable: true,
      }
    });
  });
</script>
{% schema %}
{
  "name": "Giiker Home Favourite",
  "class": "giiker-home-favourite-content",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "color",
      "id": "background_color",
      "label": "Background Color"
    }
  ],
  "blocks": [
    {
      "type": "banner",
      "name": "Banner",
      "settings": [
        {
          "type": "image_picker",
          "id": "product_image",
          "label": "product_image 240*240 推荐尺寸"
        },
        {
          "type": "text",
          "id": "product_title",
          "label": "Product Title"
        },
        {
          "type": "product",
          "id": "product",
          "label": "Product"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Giiker Home Favourite"
    }
  ]
}
{% endschema %}